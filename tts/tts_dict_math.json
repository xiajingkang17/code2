{
  "name": "math",
  "description": "High school math pronunciation tweaks",
  "raw": {
    "regex": [
      {"pattern": "\\\\frac\\{d\\}\\{d([A-Za-z])\\}", "replace": "对 \\1 求导"},
      {"pattern": "\\\\frac\\{\\\\partial\\}\\{\\\\partial\\s*([A-Za-z])\\}", "replace": "对 \\1 偏导"},
      {"pattern": "\\\\sum_\\{([^{}=]+)=([^{}]+)\\}\\^\\{([^{}]+)\\}", "replace": "对 \\1 从 \\2 到 \\3 求和"},
      {"pattern": "\\\\sum_\\{([^{}]+)\\}\\^\\{([^{}]+)\\}", "replace": "对 \\1 到 \\2 求和"},
      {"pattern": "\\\\prod_\\{([^{}=]+)=([^{}]+)\\}\\^\\{([^{}]+)\\}", "replace": "对 \\1 从 \\2 到 \\3 连乘"},
      {"pattern": "\\\\prod_\\{([^{}]+)\\}\\^\\{([^{}]+)\\}", "replace": "对 \\1 到 \\2 连乘"},
      {"pattern": "\\\\sqrt\\[([^\\]]+)\\]\\{([^{}]+)\\}", "replace": "\\1 次根号 \\2"},
      {"pattern": "\\\\sqrt\\{([^{}]+)\\}", "replace": "根号 \\1"},
      {"pattern": "\\\\frac\\{([^{}]+)\\}\\{([^{}]+)\\}", "replace": "\\1 分之 \\2"},
      {"pattern": "\\\\log_\\{([^{}]+)\\}\\s*\\{([^{}]+)\\}", "replace": "以 \\1 为底 \\2 的对数"},
      {"pattern": "\\\\log_\\{([^{}]+)\\}\\s*([A-Za-z0-9]+)", "replace": "以 \\1 为底 \\2 的对数"},
      {"pattern": "\\\\log_([A-Za-z0-9]+)\\s*([A-Za-z0-9]+)", "replace": "以 \\1 为底 \\2 的对数"},
      {"pattern": "\\\\Rightarrow", "replace": "推出"},
      {"pattern": "\\\\Leftarrow", "replace": "由此可得"},
      {"pattern": "\\\\Leftrightarrow", "replace": "当且仅当"},
      {"pattern": "\\\\iff\\b", "replace": "当且仅当"},
      {"pattern": "\\\\because\\b", "replace": "因为"},
      {"pattern": "\\\\therefore\\b", "replace": "因此"},
      {"pattern": "\\\\forall\\b", "replace": "对任意"},
      {"pattern": "\\\\exists\\b", "replace": "存在"},
      {"pattern": "\\\\emptyset\\b", "replace": "空集"},
      {"pattern": "\\\\in\\b", "replace": "属于"},
      {"pattern": "\\\\notin\\b", "replace": "不属于"},
      {"pattern": "\\\\subseteq\\b", "replace": "包含于"},
      {"pattern": "\\\\subset\\b", "replace": "真包含于"},
      {"pattern": "\\\\cup\\b", "replace": "并集"},
      {"pattern": "\\\\cap\\b", "replace": "交集"},
      {"pattern": "\\\\setminus\\b", "replace": "差集"},
      {"pattern": "\\\\mathbb\\{N\\}", "replace": "自然数集"},
      {"pattern": "\\\\mathbb\\{Z\\}", "replace": "整数集"},
      {"pattern": "\\\\mathbb\\{Q\\}", "replace": "有理数集"},
      {"pattern": "\\\\mathbb\\{R\\}", "replace": "实数集"},
      {"pattern": "\\\\mathbb\\{C\\}", "replace": "复数集"},
      {"pattern": "\\\\leq\\b", "replace": "小于等于"},
      {"pattern": "\\\\geq\\b", "replace": "大于等于"},
      {"pattern": "\\\\le\\b", "replace": "小于等于"},
      {"pattern": "\\\\ge\\b", "replace": "大于等于"},
      {"pattern": "\\\\neq\\b", "replace": "不等于"},
      {"pattern": "\\\\ne\\b", "replace": "不等于"},
      {"pattern": "\\\\approx\\b", "replace": "约等于"},
      {"pattern": "\\\\simeq\\b", "replace": "约等于"},
      {"pattern": "\\\\cdot\\b", "replace": "乘"},
      {"pattern": "\\\\times\\b", "replace": "乘"},
      {"pattern": "\\\\pm\\b", "replace": "正负"},
      {"pattern": "\\\\mp\\b", "replace": "负正"},
      {"pattern": "\\\\infty\\b", "replace": "无穷大"},
      {"pattern": "\\\\lim\\b", "replace": "极限"},
      {"pattern": "\\\\int\\b", "replace": "积分"},
      {"pattern": "\\\\sin\\b", "replace": "正弦"},
      {"pattern": "\\\\cos\\b", "replace": "余弦"},
      {"pattern": "\\\\tan\\b", "replace": "正切"},
      {"pattern": "\\\\cot\\b", "replace": "余切"},
      {"pattern": "\\\\ln\\b", "replace": "自然对数"},
      {"pattern": "\\\\log\\b", "replace": "对数"},
      {"pattern": "([A-Za-z])\\^\\{([^{}]+)\\}", "replace": "\\1 的 \\2 次方"},
      {"pattern": "([A-Za-z])\\^([A-Za-z0-9]+)", "replace": "\\1 的 \\2 次方"},
      {"pattern": "([A-Za-z])_\\{([^{}]+)\\}", "replace": "\\1 下标 \\2"},
      {"pattern": "([A-Za-z])_([A-Za-z0-9]+)", "replace": "\\1 下标 \\2"},
      {"pattern": "\\\\angle\\s*([A-Za-z0-9]+)", "replace": "角 \\1"},
      {"pattern": "\\\\triangle\\s*([A-Za-z0-9]+)", "replace": "三角形 \\1"},
      {"pattern": "\\\\overline\\{([^{}]+)\\}", "replace": "\\1 线段"},
      {"pattern": "\\\\vec\\{([^{}]+)\\}", "replace": "向量 \\1"},
      {"pattern": "\\\\overrightarrow\\{([^{}]+)\\}", "replace": "向量 \\1"},
      {"pattern": "\\\\parallel\\b", "replace": "平行"},
      {"pattern": "\\\\perp\\b", "replace": "垂直"},
      {"pattern": "\\\\cong\\b", "replace": "全等"},
      {"pattern": "\\\\sim\\b", "replace": "相似"},
      {"pattern": "\\|([^|]+)\\|", "replace": "\\1 的绝对值"}
    ]
  },
  "speech": {
    "literal": {
      "∴": "因此",
      "∵": "因为",
      "⇒": "推出",
      "⇔": "当且仅当",
      "≈": "约等于",
      "≠": "不等于",
      "≤": "小于等于",
      "≥": "大于等于",
      "∈": "属于",
      "∉": "不属于",
      "∅": "空集",
      "∀": "对任意",
      "∃": "存在",
      "∥": "平行",
      "⊥": "垂直",
      "×": "乘",
      "·": "乘",
      "/": "除以",
      "=": "等于"
    }
  }
}
